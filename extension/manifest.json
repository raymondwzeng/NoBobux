{
    "name":"NoBobux",
    "version": "0.0.0.2",
    "manifest_version": 2,
    "description": "Hides your Robux count on Roblox websites. That's it.",
    "author": "MacAndSwiss",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png"
    },
    "content_scripts": [{
        "matches": ["*://*.roblox.com/*"],
        "js": ["content.js"],
        "run_at":"document_idle"
    }],
    "permissions":["https://*.roblox.com/"]
}